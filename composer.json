{
  "name": "h4kuna/data-type",
  "type": "library",
  "description": "Basic data type, int, float, string, set, gps",
  "homepage": "https://github.com/h4kuna/data-type",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/utils": ">=2.2"
  },
  "autoload": {
    "psr-4": {
      "h4kuna\\DataType\\": "src/"
    },
    "files": [
      "src/exceptions.php",
      "src/functions.php"
    ]
  },
  "require-dev": {
    "salamium/testinium": "^0.1",
    "phpstan/phpstan": "^1.3"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpstan": [
      "vendor/bin/phpstan analyse --level max src tests"
    ]
  }
}
